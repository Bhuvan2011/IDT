<button class="add-user-button">Add User</button>

<table id="products">
<thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>UserName</th>
    <th>Email</th>
    <th>Action</th>
  </tr>
</thead>
<tbody>
  <tr>
    <td><input id="first_name" type="text" value="" /></td>
    <td><input id="last_name" type="text" value="" /></td>
    <td><input id="user_name" type="text" value="" /></td>
    <td><input id="email" type="text" value="" /></td>
    <td>
      <button class="add-user">Add</button>
    </td>
  </tr>
  <% for user in @users: %>
    <tr>
      <td><%= user.attributes.first_name %></td>
      <td><%= user.attributes.last_name %></td>
      <td><%= user.attributes.user_name %></td>
      <td><%= user.attributes.email %></td> 
      <td>
        <button class="delete-user" data-user-id="<%= user.attributes.id%>">Remove</button>
      </td>
    </tr>
  <% end %>
</tbody>
</table>
